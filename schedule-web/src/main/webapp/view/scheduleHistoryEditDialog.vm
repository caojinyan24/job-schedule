<div class="panel-body">
    <form class="form-horizontal" role="form" id="dailogForm" action="../scheduleHistory/scheduleHistoryEdit.do"
          method="POST">
        <input type="hidden" id="btn_sub" class="btn_sub"/>
        <input type="hidden" value="$!{scheduleHistory.id}" name="id" id="id"/>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">任务名</label>
            <div class="col-sm-8">
                <input type="text" value="$!{scheduleHistory.jobName}" name="jobName" id="jobName" class="form-control"
                       placeholder="任务名"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">调度地址</label>
            <div class="col-sm-8">
                <input type="text" value="$!{scheduleHistory.scheduleAddress}" name="scheduleAddress"
                       id="scheduleAddress" class="form-control" placeholder="调度地址"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">调度参数</label>
            <div class="col-sm-8">
                <input type="text" value="$!{scheduleHistory.scheduleParam}" name="scheduleParam" id="scheduleParam"
                       class="form-control" placeholder="调度参数"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">执行时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!{scheduleHistory.executeTime}" name="executeTime" id="executeTime"
                       class="form-control" placeholder="执行时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label"
                   style="text-align:left;">执行状态：0-未执行，1-失败，2-成功</label>
            <div class="col-sm-8">
                <input type="text" value="$!{scheduleHistory.executeStatus}" name="executeStatus" id="executeStatus"
                       class="form-control" placeholder="执行状态：0-未执行，1-失败，2-成功"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">创建时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{scheduleHistory.createTime})"
                       name="createTime" id="createTime" class="form-control" placeholder="创建时间"/>
            </div>
        </div>
        <div class="form-group mno">
            <label for="inputEmail3" class="col-sm-2 control-label" style="text-align:left;">更新时间</label>
            <div class="col-sm-8">
                <input type="text" value="$!dateTool.format("yyyy-MM-dd",$!{scheduleHistory.updateTime})"
                       name="updateTime" id="updateTime" class="form-control" placeholder="更新时间"/>
            </div>
        </div>
    </form>
</div>
<script type="text/javascript" src="/resources/js/Validform_v5.3.2.js"></script>
<script type="text/javascript" src="/resources/js/forminit.js"></script> 