<form role="form" class="form-inline" action="../job/jobIndex.do" method="post" id="formSubmit">
    <div class="col-md-10" style="width:100%">
        <div class="panel panel-default">
            <div class="panel-heading">任务表列表</div>
            <div class="panel-body">
                <div class="search">
                                <div class="form-group col-sm-3">
                                    <label for="jobName"
                                           class="control-label col-sm-3 line34">任务名</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="jobName"
                                               id="jobName"
                                               value="$!{job.jobName}" class="form-control"
                                               placeholder="任务名">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="appName"
                                           class="control-label col-sm-3 line34">应用名</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="appName"
                                               id="appName"
                                               value="$!{job.appName}" class="form-control"
                                               placeholder="应用名">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="beanName"
                                           class="control-label col-sm-3 line34">类名</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="beanName"
                                               id="beanName"
                                               value="$!{job.beanName}" class="form-control"
                                               placeholder="类名">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="methodName"
                                           class="control-label col-sm-3 line34">方法名</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="methodName"
                                               id="methodName"
                                               value="$!{job.methodName}" class="form-control"
                                               placeholder="方法名">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="cronParam"
                                           class="control-label col-sm-3 line34">参数</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="cronParam"
                                               id="cronParam"
                                               value="$!{job.cronParam}" class="form-control"
                                               placeholder="参数">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="createTime"
                                           class="control-label col-sm-3 line34">创建时间</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="createTime"
                                               id="createTime"
                                               value="$!dateTool.format('yyyy-MM-dd',$!{job.createTime})"
                                               class="form-control" placeholder="创建时间">
                                    </div>
                                </div>
                                <div class="form-group col-sm-3">
                                    <label for="updateTime"
                                           class="control-label col-sm-3 line34">更新时间</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="updateTime"
                                               id="updateTime"
                                               value="$!dateTool.format('yyyy-MM-dd',$!{job.updateTime})"
                                               class="form-control" placeholder="更新时间">
                                    </div>
                                </div>
                    <div class="clearfix"></div>
                    <button type="submit" class="btn btn-primary">搜 索</button>
                </div>
                <div id="legend">
                    <legend class="le">
                        <button type="button" class="btn btn-primary"
                                onclick="remoteUrl('../job/toAddDialog.do','新增用户')">新增
                        </button>
                    </legend>
                </div>
                <table class="table table-striped">
                    <thead>
                            <th>任务名</th>
                            <th>应用名</th>
                            <th>类名</th>
                            <th>方法名</th>
                            <th>参数</th>
                            <th>创建时间</th>
                            <th>更新时间</th>
                    <th>操作</th>
                    </thead>
                    <tobody>
                        #if($!{pageInfos})
                        #foreach($!{info} in $!{pageInfos})
                        <tr>
                                        <td>$!{info.jobName}</td>
                                        <td>$!{info.appName}</td>
                                        <td>$!{info.beanName}</td>
                                        <td>$!{info.methodName}</td>
                                        <td>$!{info.cronParam}</td>
                                        <td>$!dateTool.format("yyyy-MM-dd",$!{info.createTime})</td>
                                        <td>$!dateTool.format("yyyy-MM-dd",$!{info.updateTime})</td>
                            <td class="last">
                                <a href="javascript:remoteUrl('../job/toEditDialog.do?id=$!{info.id}','编辑用户')">编辑</a>
                                <a href="javascript:delData('../job/jobDelete.do?id=$!{info.id}')">删除</a>
                                <a href="../job/jobDetail.do?id=$!{info.id}">详情</a>
                            </td>
                        </tr>
                        #end
                        #end
                    </tobody>
                </table>
                <div class="text-right">
                    <!--公用翻页代码-->
                    #set($attr='formSubmit')
                    #showPageList($pageInfos $attr)
                    <!--END公用翻页代码-->
                </div>
            </div>
        </div>
    </div>
</form>
#parse("common/modal.vm")

<script type="text/javascript">
    $(document).ready(function () {
        $("#addButton").bind("click", function () {
            location.href = "../job/toAdd.do";
        });
        $("#dialogFormSubmit").bind("click", function () {
            $("#btn_sub").click();
        });
    });

</script>