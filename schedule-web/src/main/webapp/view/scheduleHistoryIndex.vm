<form role="form" class="form-inline" action="../scheduleHistory/scheduleHistoryIndex.do" method="post" id="formSubmit">
    <div class="col-md-10" style="width:100%">
        <div class="panel panel-default">
            <div class="panel-heading">调度历史列表</div>
            <div class="panel-body">
                <div class="search">
                    <div class="form-group col-sm-3">
                        <label for="jobName"
                               class="control-label col-sm-3 line34">任务名</label>
                        <div class="col-sm-8">
                            <input type="text" name="jobName"
                                   id="jobName"
                                   value="$!{scheduleHistory.jobName}" class="form-control"
                                   placeholder="任务名">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="scheduleAddress"
                               class="control-label col-sm-3 line34">调度地址</label>
                        <div class="col-sm-8">
                            <input type="text" name="scheduleAddress"
                                   id="scheduleAddress"
                                   value="$!{scheduleHistory.scheduleAddress}" class="form-control"
                                   placeholder="调度地址">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="scheduleParam"
                               class="control-label col-sm-3 line34">调度参数</label>
                        <div class="col-sm-8">
                            <input type="text" name="scheduleParam"
                                   id="scheduleParam"
                                   value="$!{scheduleHistory.scheduleParam}" class="form-control"
                                   placeholder="调度参数">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="executeTime"
                               class="control-label col-sm-3 line34">执行时间</label>
                        <div class="col-sm-8">
                            <input type="text" name="executeTime"
                                   id="executeTime"
                                   value="$!{scheduleHistory.executeTime}" class="form-control"
                                   placeholder="执行时间">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="executeStatus"
                               class="control-label col-sm-3 line34">执行状态：0-未执行，1-失败，2-成功</label>
                        <div class="col-sm-8">
                            <input type="text" name="executeStatus"
                                   id="executeStatus"
                                   value="$!{scheduleHistory.executeStatus}" class="form-control"
                                   placeholder="执行状态：0-未执行，1-失败，2-成功">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="createTime"
                               class="control-label col-sm-3 line34">创建时间</label>
                        <div class="col-sm-8">
                            <input type="text" name="createTime"
                                   id="createTime"
                                   value="$!dateTool.format('yyyy-MM-dd',$!{scheduleHistory.createTime})"
                                   class="form-control" placeholder="创建时间">
                        </div>
                    </div>
                    <div class="form-group col-sm-3">
                        <label for="updateTime"
                               class="control-label col-sm-3 line34">更新时间</label>
                        <div class="col-sm-8">
                            <input type="text" name="updateTime"
                                   id="updateTime"
                                   value="$!dateTool.format('yyyy-MM-dd',$!{scheduleHistory.updateTime})"
                                   class="form-control" placeholder="更新时间">
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <button type="submit" class="btn btn-primary">搜 索</button>
                </div>
                <div id="legend">
                    <legend class="le">
                        <button type="button" class="btn btn-primary"
                                onclick="remoteUrl('../scheduleHistory/toAddDialog.do','新增用户')">新增
                        </button>
                    </legend>
                </div>
                <table class="table table-striped">
                    <thead>
                    <th>任务名</th>
                    <th>调度地址</th>
                    <th>调度参数</th>
                    <th>执行时间</th>
                    <th>执行状态：0-未执行，1-失败，2-成功</th>
                    <th>创建时间</th>
                    <th>更新时间</th>
                    <th>操作</th>
                    </thead>
                    <tobody>
                        #if($!{pageInfos})
                            #foreach($!{info} in $!{pageInfos})
                                <tr>
                                    <td>$!{info.jobName}</td>
                                    <td>$!{info.scheduleAddress}</td>
                                    <td>$!{info.scheduleParam}</td>
                                    <td>$!{info.executeTime}</td>
                                    <td>$!{info.executeStatus}</td>
                                    <td>$!dateTool.format("yyyy-MM-dd",$!{info.createTime})</td>
                                    <td>$!dateTool.format("yyyy-MM-dd",$!{info.updateTime})</td>
                                    <td class="last">
                                        <a href="javascript:remoteUrl('../scheduleHistory/toEditDialog.do?id=$!{info.id}','编辑用户')">编辑</a>
                                        <a href="javascript:delData('../scheduleHistory/scheduleHistoryDelete.do?id=$!{info.id}')">删除</a>
                                        <a href="../scheduleHistory/scheduleHistoryDetail.do?id=$!{info.id}">详情</a>
                                    </td>
                                </tr>
                            #end
                        #end
                    </tobody>
                </table>
                <div class="text-right">
                    <!--公用翻页代码-->
                    #set($attr='formSubmit')
                    #showPageList($pageInfos $attr)
                    <!--END公用翻页代码-->
                </div>
            </div>
        </div>
    </div>
</form>
#parse("common/modal.vm")

<script type="text/javascript">
    $(document).ready(function () {
        $("#addButton").bind("click", function () {
            location.href = "../scheduleHistory/toAdd.do";
        });
        $("#dialogFormSubmit").bind("click", function () {
            $("#btn_sub").click();
        });
    });

</script>